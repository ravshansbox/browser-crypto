{
  "name": "browser-crypto",
  "version": "1.0.1",
  "description": "Node.js crypto module subset for browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravshansbox/browser-crypto.git"
  },
  "license": "MIT",
  "keywords": [
    "browser",
    "crypto",
    "aes",
    "cipher"
  ],
  "main": "dist/browser-crypto.js",
  "scripts": {
    "build": "webpack --mode none --output-filename browser-crypto.js",
    "build:min": "webpack --mode production --output-filename browser-crypto.min.js",
    "prepublish": "npm run build && npm run build:min",
    "start": "webpack -dw"
  },
  "devDependencies": {
    "browserify-aes": "~1.2.0",
    "create-ecdh": "~4.0.0",
    "safe-buffer": "~5.2.0",
    "webpack": "~4.39.0",
    "webpack-cli": "~3.3.0"
  },
  "files": [
    "dist/browser-crypto.js",
    "dist/browser-crypto.min.js"
  ]
}
